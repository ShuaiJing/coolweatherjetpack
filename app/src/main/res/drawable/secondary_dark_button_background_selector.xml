<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 点击 -->
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="5dp" />
            <stroke android:width="1dp" android:color="@color/text_color_ffa3a3a3" />
        </shape>
    </item>
    <!-- 不可用 -->
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="5dp" />
            <stroke android:width="1dp" android:color="@color/text_color_ffa3a3a3" />
        </shape>
    </item>
    <!-- 默认 -->
    <item>
        <shape>
            <corners android:radius="5dp" />
            <stroke android:width="1dp" android:color="@color/text_color_ff666666" />
        </shape>
    </item>
</selector>